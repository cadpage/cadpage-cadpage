<?xml version="1.0" encoding="utf-8"?>
<!--suppress AndroidDomInspection -->
<PreferenceScreen
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:android="http://schemas.android.com/apk/res/android">

  <Preference
    app:key="@string/pref_loc_desc_key"
    app:title="@string/pref_loc_desc_title"/>

  <PreferenceScreen
    app:key="@string/pref_location_tree_key"
    app:title="@string/pref_location_title"
    app:summary="@string/pref_location_summary"/>

  <PreferenceScreen
    app:key="@string/pref_location_mtree_key"
    app:title="@string/pref_multi_location_title"
    app:summary="@string/pref_multi_location_summary"/>

  <SwitchPreference
    app:key="@string/pref_override_filter_key"
    app:title="@string/pref_override_filter_title"
    app:summaryOn="@string/pref_override_filter_summaryon"
    app:summaryOff="@string/pref_override_filter_summaryoff"
    app:defaultValue="@string/pref_override_filter_default" />

  <net.anei.cadpage.preferences.EditTextPreference
    app:dialogTitle="@string/pref_filter_title"
    app:dialogMessage="@string/pref_filter_dialog"
    app:key="@string/pref_filter_key"
    app:title="@string/pref_filter_title"
    app:summary="@string/pref_filter_summary"
    app:defaultValue="" />

  <SwitchPreference
    app:key="@string/pref_show_source_key"
    app:title="@string/pref_show_source_title"
    app:summaryOn="@string/pref_show_source_summaryon"
    app:summaryOff="@string/pref_show_source_summaryoff"
    app:defaultValue="@string/pref_show_source_default" />

  <SwitchPreference
    app:key="@string/pref_override_default_key"
    app:title="@string/pref_override_default_title"
    app:summaryOn="@string/pref_override_default_summaryon"
    app:summaryOff="@string/pref_override_default_summaryoff"
    app:defaultValue="@string/pref_override_default_default" />

  <net.anei.cadpage.preferences.EditTextPreference
    app:dialogTitle="@string/pref_defcity_title"
    app:dialogMessage="@string/pref_defcity_dialog"
    app:key="@string/pref_defcity_key"
    app:title="@string/pref_defcity_title"
    app:summary="@string/pref_defcity_summary"
    android:inputType="textCapCharacters"
    app:defaultValue="" />

  <net.anei.cadpage.preferences.EditTextPreference
    app:dialogTitle="@string/pref_defstate_title"
    app:dialogMessage="@string/pref_defstate_dialog"
    app:key="@string/pref_defstate_key"
    app:title="@string/pref_defstate_title"
    app:summary="@string/pref_defstate_summary"
    android:inputType="textCapCharacters"
    app:defaultValue="" />

  <net.anei.cadpage.preferences.ListPreference
    app:key="@string/pref_enable_msg_type_key"
    app:title="@string/pref_enable_msg_type_title"
    app:summary="@string/pref_enable_msg_type_summary"
    app:entries="@array/pref_enable_msg_type_entries"
    app:entryValues="@array/pref_enable_msg_type_values"
    app:defaultValue="@string/pref_enable_msg_type_default" />

  <SwitchPreference
    app:key="@string/pref_use_old_mms_key"
    app:title="@string/pref_use_old_mms_title"
    app:summaryOn="@string/pref_use_old_mms_summaryon"
    app:summaryOff="@string/pref_use_old_mms_summaryoff"
    app:defaultValue="false" />

  <net.anei.cadpage.preferences.ListPreference
    app:key="@string/pref_mms_timeout_key"
    app:title="@string/pref_mms_timeout_title"
    app:summary="@string/pref_mms_timeout_summary"
    app:entries="@array/pref_mms_timeout_values"
    app:entryValues="@array/pref_mms_timeout_values"
    app:dialogTitle="@string/pref_mms_timeout_title"
    app:defaultValue="@string/pref_mms_timeout_default"/>

  <net.anei.cadpage.preferences.ListPreference
    app:key="@string/pref_loglimit_key"
    app:title="@string/pref_loglimit_title"
    app:summary="@string/pref_loglimit_summary"
    app:entries="@array/pref_loglimit_values"
    app:entryValues="@array/pref_loglimit_values"
    app:dialogTitle="@string/pref_loglimit_title"
    app:defaultValue="@string/pref_loglimit_default"/>

  <Preference
    app:fragment="net.anei.cadpage.PreferenceSplitMergeOptionsFragment"
    app:title="@string/pref_split_msg_merge_title"
    app:summary="@string/pref_split_msg_merge_summary"/>

  <SwitchPreference
    app:key="@string/pref_suppress_dup_msg_key"
    app:title="@string/pref_suppress_dup_msg_title"
    app:summaryOn="@string/pref_suppress_dup_msg_summaryon"
    app:summaryOff="@string/pref_suppress_dup_msg_summaryoff"
    app:defaultValue="@string/pref_suppress_dup_msg_default" />

  <SwitchPreference
    app:key="@string/pref_override_vendor_loc_key"
    app:title="@string/pref_override_vendor_loc_title"
    app:summaryOn="@string/pref_override_vendor_loc_summaryon"
    app:summaryOff="@string/pref_override_vendor_loc_summaryoff"
    app:defaultValue="@string/pref_override_vendor_loc_default" />

  <SwitchPreference
    app:key="@string/pref_activate_scanner_key"
    app:title="@string/pref_activate_scanner_title"
    app:summaryOn="@string/pref_activate_scanner_summaryon"
    app:summaryOff="@string/pref_activate_scanner_summaryoff"
    app:defaultValue="@string/pref_activate_scanner_default" />

  <Preference
    app:key="@string/pref_scanner_channel_key"
    app:title="@string/pref_scanner_channel_title" />

  <net.anei.cadpage.preferences.ScannerTimeoutPreference
    app:dialogTitle="@string/pref_scanner_timeout_title"
    app:dialogMessage="@string/pref_scanner_timeout_dialog"
    app:key="@string/pref_scanner_timeout_key"
    app:title="@string/pref_scanner_timeout_title"
    app:summary="@string/pref_scanner_timeout_summary"
    android:inputType="number"
    app:defaultValue="@string/pref_scanner_timeout_default"/>

  <SwitchPreference
    app:key="@string/pref_publish_pages_key"
    app:title="@string/pref_publish_pages_title"
    app:summaryOn="@string/pref_publish_pages_summaryon"
    app:summaryOff="@string/pref_publish_pages_summaryoff"
    app:defaultValue="@string/pref_publish_pages_default" />

</PreferenceScreen>