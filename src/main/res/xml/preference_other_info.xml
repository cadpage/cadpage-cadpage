<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
  xmlns:android="http://schemas.android.com/apk/res/android">

  <PreferenceScreen
  android:key="@string/pref_disclaimer_key" android:title="@string/pref_disclaimer_title" android:summary="@string/pref_disclaimer_summary">
    <intent
    android:action="android.intent.action.VIEW" android:data="@string/pref_disclaimer_url"/>
  </PreferenceScreen>
  <PreferenceScreen
  android:key="@string/pref_privacy_policy_key" android:title="@string/pref_privacy_policy_title" android:summary="@string/pref_privacy_policy_summary">
    <intent
    android:action="android.intent.action.VIEW" android:data="@string/pref_privacy_policy_url"/>
  </PreferenceScreen>

  <PreferenceScreen
  android:key="@string/pref_faq_key"
  android:title="@string/pref_faq_title"
  android:summary="@string/pref_faq_summary">
    <intent
    android:action="android.intent.action.VIEW"
    android:data="@string/pref_faq_url" />
  </PreferenceScreen>

  <net.anei.cadpage.preferences.DialogPreference
  android:negativeButtonText="@null"
  android:key="@string/pref_about_key"
  android:title="@string/pref_about_title"
  android:dialogIcon="@drawable/ic_launcher"
  android:summary="@string/pref_about_summary"
  android:dialogTitle="@string/app_name"/>

  <net.anei.cadpage.preferences.DialogPreference
  android:key="@string/pref_releasenotes_key"
  android:negativeButtonText="@null"
  android:title="@string/pref_releasenotes_title"
  android:summary="@string/pref_releasenotes_summary"
  android:dialogIcon="@drawable/ic_launcher"
  android:dialogTitle="@string/pref_releasenotes_diagtitle"
  android:dialogMessage="@string/release_text"/>

  <PreferenceScreen
  android:title="@string/pref_cadpage_active911_title"
  android:summary="@string/pref_cadpage_active911_summary">
    <intent
    android:action="android.intent.action.VIEW"
    android:data="@string/pref_cadpage_active911_url" />
  </PreferenceScreen>

</PreferenceScreen>