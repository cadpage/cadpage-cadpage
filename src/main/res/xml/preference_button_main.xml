<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
  xmlns:app="http://schemas.android.com/apk/res-auto">
  app:title="@string/pref_main_button_config_title"
  app:summary="@string/pref_main_button_config_summary">
  <net.anei.cadpage.preferences.ListPreference
    app:key="@string/pref_button1_key"
    app:title="@string/pref_button1_title"
    app:summary="@string/pref_button_summary"
    app:entries="@array/pref_button_text"
    app:entryValues="@array/pref_button_values"
    app:dialogTitle="@string/pref_button1_title"
    app:defaultValue="@string/pref_button1_default"/>
  <net.anei.cadpage.preferences.ListPreference
    app:key="@string/pref_button2_key"
    app:title="@string/pref_button2_title"
    app:summary="@string/pref_button_summary"
    app:entries="@array/pref_button_text"
    app:entryValues="@array/pref_button_values"
    app:dialogTitle="@string/pref_button2_title"
    app:defaultValue="@string/pref_button2_default"/>
  <net.anei.cadpage.preferences.ListPreference
    app:key="@string/pref_button3_key"
    app:title="@string/pref_button3_title"
    app:summary="@string/pref_button_summary"
    app:entries="@array/pref_button_text"
    app:entryValues="@array/pref_button_values"
    app:dialogTitle="@string/pref_button3_title"
    app:defaultValue="@string/pref_button3_default"/>
  <net.anei.cadpage.preferences.ListPreference
    app:key="@string/pref_button4_key"
    app:title="@string/pref_button4_title"
    app:summary="@string/pref_button_summary"
    app:entries="@array/pref_button_text"
    app:entryValues="@array/pref_button_values"
    app:dialogTitle="@string/pref_button4_title"
    app:defaultValue="@string/pref_button4_default"/>
  <net.anei.cadpage.preferences.ListPreference
    app:key="@string/pref_button5_key"
    app:title="@string/pref_button5_title"
    app:summary="@string/pref_button_summary"
    app:entries="@array/pref_button_text"
    app:entryValues="@array/pref_button_values"
    app:dialogTitle="@string/pref_button5_title"
    app:defaultValue="@string/pref_button5_default"/>
  <net.anei.cadpage.preferences.ListPreference
    app:key="@string/pref_button6_key"
    app:title="@string/pref_button6_title"
    app:summary="@string/pref_button_summary"
    app:entries="@array/pref_button_text"
    app:entryValues="@array/pref_button_values"
    app:dialogTitle="@string/pref_button6_title"
    app:defaultValue="@string/pref_button6_default"/>
  <SwitchPreference
    app:key="@string/pref_alt_map_button_key"
    app:title="@string/pref_alt_map_button_title"
    app:summaryOn="@string/pref_alt_map_button_summaryon"
    app:summaryOff="@string/pref_alt_map_button_summaryoff"
    app:defaultValue="true" />
  <SwitchPreference
    app:key="@string/pref_map_page_button_key"
    app:title="@string/pref_map_page_button_title"
    app:summaryOn="@string/pref_map_page_button_summaryon"
    app:summaryOff="@string/pref_map_page_button_summaryoff"
    app:defaultValue="true" />
  <SwitchPreference
    app:key="@string/pref_enable_return_call_key"
    app:title="@string/pref_enable_return_call_title"
    app:summaryOn="@string/pref_enable_return_call_summaryon"
    app:summaryOff="@string/pref_enable_return_call_summaryoff"
    app:defaultValue="true" />
</PreferenceScreen>